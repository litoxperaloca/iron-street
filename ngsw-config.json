{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icon/favicon.png",
          "/assets/icon/shapes.svg",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/*.json",
          "/assets/utils/mapbox/mapbox-gl.css",
          "/assets/utils/mapbox/mapbox-gl-directions.css",
          "/assets/utils/threebox/threebox.js",
          "/assets/utils/threebox/threebox.css",
          "/assets/i18n/*.json",
          "/assets/models/**",
          "/assets/models/*.glb",
          "/assets/models/*.gltf"
         ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",

          "/assets/img/**",
          "/assets/img/cars/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)",
          "/assets/img/motorbikes/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)",
          "/assets/img/icons/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)",
          "/assets/img/map-icons/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)",
          "/assets/img/map-controls/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)",
          "/assets/img/map-styles/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)",
          "/assets/fonts/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)",
          "/assets/about/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2|glb|gltf)"
        ]
      }
    }
  ]
}
